Material "Builtin/DeferredLighting"
{
	Pass
	{
		Tag "Default"
		Shader "Builtin/DeferredLighting/Default"
		{
			#comp CSMain
			#keyword MAIN_LIGHT_MODE : 2
			#define MAIN_LIGHT_MODE_NONE        0
			#define MAIN_LIGHT_MODE_POINT       1
			#define MAIN_LIGHT_MODE_DIRECTIONAL 2
			#include "Lighting.hlsl"
		}
	}

	Pass
	{
		Tag "Sky"
		Shader "Builtin/DeferredLighting/Sky"
		{
			#comp CSMain
			#include "Sky.hlsl"
		}
	}
}
